			<div class="row justify-content-around w-100 ms-4">
                <div class="col-1 justify-content-start align-items-center pe-0">
					<a class="navbar-link" asp-area="" asp-controller="Home" asp-action="Index">
						<img style="width: 70px; height: 67px" src="data:image/png;base64,"
							 alt="Alternate Text" />
					</a>
                </div>
				
                <div class="col-6 justify-content-start align-content-center ps-0">
                   
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse ps-0" id="navbarNav">
						<ul class="navbar-nav ">
							<li class="nav-item">
								<a class="nav-link ps-0 pe-3" asp-area="" asp-controller="Product" asp-action="Index">Shop</a>
							</li>
							<li class="nav-item">
								<a class="nav-link px-3" asp-area="" asp-controller="Cart" asp-action="Index">Carrello</a>
							</li>
							<li class="nav-item">
								<a class="nav-link px-3" asp-area="" asp-controller="Product" asp-action="Admin">Admin</a>
							</li>
						</ul>
                    </div>
				</div>
               
				<div class="col-4 d-flex justify-content-center align-content-center">
					

                        <form class="w-50  d-flex" asp-controller="Product" asp-action="Index" method="get">
                            <input class="search-input w-100 ps-5" type="search" name="searchTerm" placeholder="Cerca prodotti..." aria-label="Search" value="@ViewBag.SearchTerm" />
                            <button class="btn btn-outline-dark rounded-0 py-1 ms-1" type="submit">Cerca</button>
